#!/bin/sh
#
# SP Racing H7 EXTREME specific board sensors init
#------------------------------------------------------------------------------
adc start

# Internal SPI bus ICM-20689
mpu6000 -R 8 -z -T 20689 start

# Possible external compasses
ist8310 -b 1 start
ist8310 -b 2 start
hmc5883 -C -T -X start
qmc5883 -X start
lis3mdl -X start

# Possible internal compass
ist8310 -b 5 start

# Baro on internal SPI
ms5611 -s start
