#!/bin/sh
#
# SP Racing H7 EXTREME specific board sensors init
#------------------------------------------------------------------------------
adc start

# Internal SPI bus ICM-20602
mpu6000 -R 8 -s -T 20602 start

# Possible external compasses
ist8310 -b 1 start
ist8310 -b 2 start
hmc5883 -C -T -X start
qmc5883 -X start
lis3mdl -X start

