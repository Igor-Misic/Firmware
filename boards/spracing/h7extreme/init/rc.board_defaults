#!/bin/sh
#
# SP Racing H7 EXTREME specific board defaults
#------------------------------------------------------------------------------

if param compare SYS_AUTOSTART 0
then
	# Select the Generic 250 Racer by default
	param set SYS_AUTOSTART 4050
	set AUTOCNF yes
fi

if [ $AUTOCNF = yes ]
then
	# Enable IMU thermal control
	param set SENS_EN_THERMAL 1
fi

set LOGGER_BUF 64
